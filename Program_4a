import matplotlib.pyplot as plt
# Sample data
categories = ['Category A', 'Category B', 'Category C', 'Category D’]
values = [25, 40, 30, 35]
# Creating a bar plot
plt.bar(categories, values, color='skyblue’)
# Adding labels and Title
plt.xlabel('Categories’)
plt.ylabel('Values’)
plt.title('Bar Plot Example’)
# Display the plot 
plt.show()
